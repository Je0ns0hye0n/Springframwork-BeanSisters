<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
 PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
   <!-- 클래스에 별명 설정 -->
   <typeAliases>
      <typeAlias type="board.bean.BoardDTO" alias="boardDTO"/>
      <typeAlias type="member.bean.MemberDTO" alias="memberDTO"/>
      <typeAlias type="cart.bean.CartDTO" alias="cartDTO"/>
      <typeAlias type="subpay.bean.SubPayDTO" alias="subPayDTO"/>

   </typeAliases>

   <!-- SQL Mapper 설정 -->
   <mappers>
      <mapper resource="mybatis/board-mapping.xml"/>
      <mapper resource="mybatis/member-mapping.xml"/>
      <mapper resource="mybatis/cart-mapping.xml"/>
      <mapper resource="mybatis/subpay-mapping.xml"/>
       
   </mappers>
</configuration>